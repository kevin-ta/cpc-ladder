{% extends '::base.html.twig' %}

{% block body %}
    <div class="col-md-12 text-center">
        {% if error is defined %}
            <div class="alert alert-danger"><p>{{ error }}</p></div>
        {% endif %}
        {% if success is defined %}
            <div class="alert alert-success"><p>{{ success }}</p></div>
        {% endif %}
    </div>
{% endblock %}